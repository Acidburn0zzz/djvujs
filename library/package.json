{
  "name": "DjVu.js_Library",
  "scripts": {
    "start": "node server.js",
    "watch": "rollup --config --watch",
    "build": "rollup --config",
    "install": "npm install && npm run browserify_pngjs",
    "browserify_pngjs": "npx browserify node_modules/pngjs/lib/png.js --standalone png > node_modules/pngjs/browser.js"
  },
  "devDependencies": {
    "colors": "^1.3.2",
    "rollup": "^0.66.1",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "ws": "^6.0.0"
  },
  "dependencies": {
    "pngjs": "^3.3.3"
  }
}
